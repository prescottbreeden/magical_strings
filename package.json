{
  "name": "magical-strings",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "global": "^4.4.0",
    "ramda": "^0.27.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.0",
    "serve": "^11.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "watch:sass": "node-sass src/sass/main.scss src/index.css -w",
    "compile:sass": "node-sass src/sass/main.scss src/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/styles.comp.css -o src/css/styles.prefixed.css",
    "compress:css": "node-sass src/styles.prefixed.css src/styles.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.5",
    "husky": "^3.0.9",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
